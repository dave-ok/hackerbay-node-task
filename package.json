{
  "name": "hackerbay_nodejs_task",
  "version": "1.0.0",
  "description": "NodeJS Task for Hackerbay Interview",
  "main": "index.js",
  "scripts": {
    "dev": "DEBUG=log NODE_ENV=dev nodemon --exec babel-node ./src/index.js",
    "start": "DEBUG=log NODE_ENV=production node ./build/index.js",
    "build": "babel src --out-dir build --delete-dir-on-start",
    "lint": "eslint \"src/**\" \"tests/**\" ",
    "lint:fix": "eslint --fix \"src/**\" \"tests/**\" ",
    "test": "NODE_ENV=test nyc --reporter=text --reporter=html mocha src/**/*.test.js",
    "pretest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dave-ok/hackerbay-nodejs-task.git"
  },
  "keywords": [
    "hackerbay",
    "nodejs"
  ],
  "author": "David Okanlawon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dave-ok/hackerbay-nodejs-task/issues"
  },
  "homepage": "https://github.com/dave-ok/hackerbay-nodejs-task#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1"
  }
}
