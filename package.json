{
  "name": "hackerbay_nodejs_task",
  "version": "1.0.0",
  "description": "NodeJS Task for Hackerbay Interview",
  "main": "index.js",
  "scripts": {
    "dev": "DEBUG=log NODE_ENV=dev nodemon --exec babel-node ./src/index.js",
    "start": "DEBUG=log NODE_ENV=production npm-run-all build start:built",
    "start:built": "DEBUG=log NODE_ENV=production node ./build/index.js",
    "build": "babel src --out-dir build --delete-dir-on-start",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "test": "NODE_ENV=test nyc --reporter=text --reporter=html mocha \"src/**/*.test.js\"",
    "test:lint": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dave-ok/hackerbay-nodejs-task.git"
  },
  "keywords": [
    "hackerbay",
    "nodejs"
  ],
  "author": "David Okanlawon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dave-ok/hackerbay-nodejs-task/issues"
  },
  "homepage": "https://github.com/dave-ok/hackerbay-nodejs-task#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "fast-json-patch": "^3.0.0-1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "sharp": "^0.27.1",
    "swagger-jsdoc": "^6.0.2",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.13",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.3.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.2.1",
    "nock": "^13.0.7",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3"
  }
}
